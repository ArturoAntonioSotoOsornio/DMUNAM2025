    "### Limpieza de datos"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "8dcbaf5c-a270-456e-928d-68d21b4117b2",
   "metadata": {},
   "outputs": [],
   "source": [
    "df.drop(columns=['name', 'ticket', 'cabin', 'boat', 'body', 'home.dest'], inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b37900e4-3ec2-44ca-bc9d-d1ffc3483020",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['age'] = df.groupby(['pclass', 'sex'])['age'].transform(lambda x: x.fillna(x.median()))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "caab96ef-ace3-45c7-96a2-2b140bae08ad",
   "metadata": {},
   "outputs": [],
   "source": [
    "df.fillna({'embarked': df['embarked'].mode()[0]}, inplace=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "bef29c23-e85e-4a13-8b26-afde94c076b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['fare'] = df.groupby('pclass')['fare'].transform(lambda x: x.fillna(x.median()))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "a057b7f1-716a-4b60-b3f1-a284653ce9c4",
