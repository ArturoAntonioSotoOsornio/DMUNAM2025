   "source": [
    "# Familia a bordo (sibsp + parch + 1)\n",
    "df['family_size'] = df['sibsp'] + df['parch'] + 1\n",
    "df['is_alone'] = (df['family_size'] == 1).astype(int)\n",
    "\n",
    "# Binario para g√©nero\n",
    "df['is_female'] = (df['sex'] == 'female').astype(int)\n",
    "\n",
    "# Binning de edad\n",
    "df['age_group'] = pd.cut(df['age'], bins=[0, 12, 30, 50, 100], labels=['child', 'young', 'adult', 'senior'])\n",
    "\n",
    "# Binning de tarifa\n",
    "df['fare_group'] = pd.qcut(df['fare'], 4, labels=['low', 'medium', 'high', 'vip'])"
   ]
  },
