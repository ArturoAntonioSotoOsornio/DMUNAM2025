   "source": [
    "def entrenar_e_imprimir_metricas(model, X_train, y_train, X_val, y_val):\n",
    "    model.fit(X_train, y_train)\n",
    "    train_score = accuracy_score(y_train, model.predict(X_train))\n",
    "    val_score = accuracy_score(y_val, model.predict(X_val))\n",
    "    return {'train': round(train_score, 3), 'validate': round(val_score, 3)}\n",
    "\n",
    "# Uso:\n",
    "metrics = entrenar_e_imprimir_metricas(model, X_train, y_train, X_val, y_val)\n",
    "print(metrics)"
   ]
